{
  "name": "protractor-flake",
  "version": "0.4.0",
  "description": "A wrapper for protractor to automatically re-run failed specs for a specific number of attempts",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/NickTomlin/protractor-flake.git"
  },
  "scripts": {
    "serve": "bin/www",
    "lint": "eslint .",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "test:unit": "mocha",
    "test:integration": "webdriver-manager update && npm run build && mocha test/helper.js test/integration/bin.js --slow 30000 --timeout 30000 --reporter tap",
    "test:dev": "mocha --watch",
    "build": "babel -d dist src",
    "prepublish": "npm run build"
  },
  "bin": "./bin/protractor-flake",
  "author": "Nick Tomlin",
  "contributors": [
    {
      "name": "Oliver Daems"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.8.20",
    "chai": "^3.2.0",
    "cookie-parser": "^1.3.5",
    "eslint": "^1.2.0",
    "express": "^4.13.1",
    "express-session": "^1.11.3",
    "mocha": "^2.2.5",
    "morgan": "^1.6.1",
    "mversion": "^1.10.0",
    "protractor": "^2.1.0",
    "proxyquire": "^1.6.0",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "minimist": "^1.1.2",
    "core-js": "^0.9.18"
  },
  "keywords": [
    "protractor",
    "flake",
    "retry"
  ]
}
